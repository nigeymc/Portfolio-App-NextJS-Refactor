esContent":["import { useState, useEffect } from \"react\";\nimport { Container, Alert } from 'react-bootstrap';\nimport HomePageTitle from \"./HomePageTitle\";\nimport SocialIcons from \"./SocialIcons\";\nimport database from '../firebase/firebase';\n\nconst HomePage = () => {\n    const [data, setData] = useState({});\n    const [error, setError] = useState(false);\n\n    useEffect(() => {\n        return database.ref()\n            .once('value')\n            .then((snapshot) => {\n                const { firstName, lastName, jobTitle, city } = snapshot.val();\n                const portfolioData = snapshot.val();\n                const { linkedIn, gitHub } = portfolioData.urls;\n                setData({ firstName, lastName, jobTitle, city, linkedIn, gitHub })\n            })\n            .catch((e) => {\n                console.error('Error fetching data', e);\n                setError(true);\n            });\n\n    }, []);\n\n    return (\n        <Container fluid className=\"p-0\">\n            <section className=\"homepage-section\" id=\"homepage\">\n                <div className=\"homepage-section__content\">\n                    {\n                        data.error && <Alert className=\"alert__error--fetch\" variant=\"info\">Error fetching page content</Alert>\n                    }\n                    <HomePageTitle firstName={data.firstName} lastName={data.lastName} jobTitle={data.jobTitle} city={data.city} />\n                    <SocialIcons linkedIn={data.linkedIn} gitHub={data.gitHub} />\n                </div>\n            </section>\n        </Container>\n    )\n}\n\nexport async function getStaticProps(context) {\n    return database.ref()\n        .once('value')\n        .then((snapshot) => {\n            const { firstName, lastName, jobTitle, city } = snapshot.val();\n            const portfolioData = snapshot.val();\n            const { linkedIn, gitHub } = portfolioData.urls;\n            return {\n                props: { firstName, lastName, jobTitle, city, linkedIn, gitHub }, // will be passed to the page component as props\n            };\n        });\n}\n\nexport { HomePage as default };\n\n\n"]},"metadata":{},"sourceType":"module"}